{
	"env": {
		"browser": true,
		"jasmine": true
	},
	"extends": "airbnb",
	"rules": {
		"max-len": [
			1,
			100,
			2,
			{
				"ignoreUrls": true,
				"ignoreComments": true,
				"ignoreRegExpLiterals": true,
				"ignoreStrings": true,
				"ignoreTemplateLiterals": true
			}
		],
		"indent": [
			2,
			"tab"
		],
		"no-unused-vars": 1,
		"no-tabs": 0,
		"import/no-named-as-default": false
	}
}
